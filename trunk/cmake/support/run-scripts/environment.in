#!/bin/sh
export @FLAVOR_UPPER@=@CMAKE_INSTALL_PREFIX@/lib/@FLAVOR@-@MCCODE_VERSION@
export PATH=@CMAKE_INSTALL_PREFIX@/lib/@FLAVOR@-@MCCODE_VERSION@/bin:$PATH
echo
echo The new shell started here is now set up for running this version of @FLAVOR@:
echo
@FLAVOR@ -v
echo
echo To end using this version of mcstas, exit this shell.
echo
export PS1='@FLAVOR@-@MCCODE_VERSION@ env \w\$ > '
if [ -x "\" ]; then
  \
else
  /bin/sh
fi
