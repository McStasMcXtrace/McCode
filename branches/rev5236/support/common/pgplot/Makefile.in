#	PGPLOT subdirectories
SRC=.
SRCDIR=./src
OBSDIR=./obssrc
DEMDIR=./examples
FNTDIR=./fonts
DRVDIR=./drivers
SYSDIR=./sys_linux
PGDDIR=./pgdispd
GENDIR=./sys
