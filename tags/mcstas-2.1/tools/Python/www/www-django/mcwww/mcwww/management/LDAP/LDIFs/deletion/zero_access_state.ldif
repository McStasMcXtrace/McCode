#============================#
# Clear initial access state #
#============================#
dn: olcDatabase={1}hdb,cn=config
changetype: modify
delete: olcAccess
olcAccess: to attrs=userPassword,shadowLastChange
  by self write by anonymous auth
  by dn="cn=admin,dc=fysik,dc=dtu,dc=dk" write
  by * none
-
delete: olcAccess
olcAccess: to dn.base=""
  by * read
-
delete: olcAccess
olcAccess: to *
  by self write
  by dn="cn=admin,dc=fysik,dc=dtu,dc=dk" write
  by * read