#!/usr/bin/env xdg-open

[Desktop Entry]
Comment=Monte-Carlo @MCCODE_PARTICLE@ instrument simulation (@FLAVOR@.org)
Exec=@MCCODE_PREFIX@gui.pl %F
Icon=/usr/share/pixmaps/@FLAVOR@-pl.png
MimeType=text/plain;text/x-csrc;
Name=@NAME@ @MCCODE_VERSION@ pl-gui (@FLAVOR@.org)
StartupNotify=true
Terminal=false
TerminalOptions=
Type=Application
X-DCOP-ServiceType=
X-KDE-SubstituteUID=false
X-KDE-Username=
Categories=Education;Applications;
Actions=Help

[Desktop Action Help]
Name=Open @FLAVOR@ Help
Exec=@MCCODE_PREFIX@doc.pl
OnlyShowIn=Unity;Gnome;
