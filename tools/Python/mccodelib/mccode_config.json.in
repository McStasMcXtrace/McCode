{
  "configuration": {
    "MCCODE_VERSION": "@MCCODE_VERSION@",
    "TERMINAL": "@TERMINAL@",
    "MCCODE": "@FLAVOR@",
    "MCRUN": "@MCCODE_PREFIX@run",
    "MCPLOT": "@MCCODE_PREFIX@plot-pyqtgraph",
    "MCDISPLAY": "@MCCODE_PREFIX@display-webgl",
    "TOOL_NAME": "@MCCODE_PREFIX@gui",
    "PARTICLE": "@MCCODE_PARTICLE@",
    "BROWSER": "@BROWSER@",
    "GUICOLS": "3",
    "EDITOR": "@EDITOR@",
    "QSCI": "@QSCI@"
  },
  "compilation": {
    "CFLAGS": "@CFLAGS@",
    "NEXUSFLAGS": "-DUSE_NEXUS -lNeXus",
    "MPIFLAGS": "-DUSE_MPI -l@MPILIB@",
    "OACCFLAGS": "-fast -Minfo=accel -acc=gpu -gpu=managed -DOPENACC",
    "CC": "@TOOLS_CC@",
    "OACC": "nvc",
    "MPICC": "@MPICC@",
    "MPIRUN": "@MPIRUN@",
    "MPINODES": "4"
  },
  "platform": {
    "EXESUFFIX": "@MCCODE_EXECUTABLE_SUFFIX@"
  }
}

